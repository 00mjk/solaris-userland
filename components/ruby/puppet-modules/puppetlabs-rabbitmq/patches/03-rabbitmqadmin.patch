In house patch for setting rabbitmqadmin to sensible Solaris defaults

--- puppetlabs-rabbitmq-5.3.1/manifests/install/rabbitmqadmin.pp.orig  2015-11-17 09:54:04.669956465 -0800
+++ puppetlabs-rabbitmq-5.3.1/manifests/install/rabbitmqadmin.pp    2015-11-17 09:55:02.467787905 -0800
@@ -25,9 +25,9 @@ class rabbitmq::install::rabbitmqadmin {
     ],
   }

-  file { '/usr/local/bin/rabbitmqadmin':
+  file { '/usr/bin/rabbitmqadmin':
     owner   => 'root',
-    group   => '0',
+    group   => 'bin',
     source  => "${rabbitmq::rabbitmq_home}/rabbitmqadmin",
     mode    => '0755',
     require => Staging::File['rabbitmqadmin'],
